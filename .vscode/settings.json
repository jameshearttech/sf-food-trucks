{
  "commitlint.config.file": "commitlint.config.mjs",
  "commitlint.globalNodePath": "/usr/local/bin/node",
  "commitlint.log.enabled": true,
  "css.format.enable": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "eslint.codeActionsOnSave.mode": "all",
  "eslint.debug": true,
  "eslint.enable": true,
  "eslint.useFlatConfig": true,
  "eslint.format.enable": false,
  "eslint.run": "onType",
  "eslint.runtime": "/usr/local/bin/node",
  "eslint.workingDirectories": [{ "mode": "auto" }],
  "files.eol": "\n",
  "html.format.enable": false,
  "javascript.format.enable": false,
  "less.format.enable": false,
  "prettier.configPath": "prettier.config.mjs",
  "prettier.endOfLine": "lf",
  "prettier.ignorePath": ".prettierignore",
  "prettier.requireConfig": true,
  "remark.requireConfig": true,
  "scss.format.enable": false,
  "typescript.format.enable": false,
  "typescript.tsserver.nodePath": "/usr/local/bin/node",
  "yaml.format.enable": false
}
